{
  "name": "@open-flash/stream",
  "version": "0.3.0",
  "homepage": "https://github.com/open-flash/stream",
  "description": "Streams for Open Flash",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-flash/stream.git"
  },
  "keywords": [
    "swf",
    "ast"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gulp lib:build",
    "watch": "gulp lib:watch",
    "test": "gulp test",
    "lint": "gulp lint",
    "format": "gulp format",
    "typedoc": "gulp lib:typedoc",
    "dist": "gulp lib:dist",
    "prepare": "gulp all:tsconfig.json && gulp tslint.json && gulp dist",
    "pretest": "gulp lint",
    "prepublishOnly": "echo \"Use \\`gulp lib:publish\\`\" && exit 1"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "engines": {
    "node": ">=13.2"
  },
  "dependencies": {
    "incident": "^3.2.0",
    "semantic-types": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/gulp": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "minimist": "^1.2.2",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "turbo-gulp": "^0.20.2",
    "typescript": "^3.7.2"
  },
  "c88": {
    "match": [
      "build/test/lib/*.{js,mjs}",
      "build/test/lib/**/*.{js,mjs}"
    ]
  }
}
